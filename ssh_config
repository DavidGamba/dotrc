KeepAlive yes
ServerAliveInterval 30
ServerAliveCountMax 6

Include work_config
Include private_config

Host *
	IgnoreUnknown UseKeychain
	AddKeysToAgent yes
	UseKeychain yes
	IdentityFile ~/.ssh/id_ed25519_work

Host github.com
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_ed25519_work

Host github.com-personal
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_ed25519_personal
